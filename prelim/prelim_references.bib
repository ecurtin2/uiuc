journal = {Nuclear Physics},
month = {nov},
pages = {225--232},
title = {{Stability conditions and nuclear rotations in the Hartree-Fock theory}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0029558260900481},
volume = {21},
year = {1960}
}
