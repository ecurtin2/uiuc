doi = {10.1021/ct500744k},
file = {:home/evan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kanters, Donald - 2014 - cluster Searching for Unique Low Energy Minima of Structures Using a Novel Implementation of a Genetic Algorit.pdf:pdf},
