issn = {0021-9606},
journal = {The Journal of Chemical Physics},
month = {mar},
number = {12},
pages = {124102},
pmid = {24697419},
title = {{Coupled cluster channels in the homogeneous electron gas}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24697419 http://scitation.aip.org/content/aip/journal/jcp/140/12/10.1063/1.4867783},
