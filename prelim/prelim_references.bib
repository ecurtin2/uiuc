doi = {10.1021/acs.jcim.5b00243},
file = {:home/evan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Supady, Blum, Baldauf - 2015 - First-Principles Molecular Structure Search with a Genetic Algorithm.pdf:pdf},
issn = {1549-9596},
